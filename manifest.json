{
  "name": "NemID AutoLogin",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Snap a picture of your ID card and we'll handle the rest.",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [""],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "*://*/*",
    "storage"
  ]
}
